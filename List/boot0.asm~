;int writePageToMemory(unsigned int address)
; 0000 022A {
; 0000 022B 
; 0000 022C #asm
;	address -> Y+0
; 0000 022D wdr
; 0000 022E clr r22
; 0000 022F clr r23
; 0000 0230 LDI  R22,0x10
; 0000 0231 mov r0, r22;
; 0000 0232 ldi r23, 0x11
; 0000 0233 mov r1, r23;
; 0000 0234 ld r30, y      ; а теперь загружем старший байт адреса
; 0000 0235 ldd r31, y+1
; 0000 0236 clr r22
; 0000 0237 LDI  R22,0x01
; 0000 0238 OUT  0x37,R22
; 0000 0239 cli
; 0000 023A spm
; 0000 023B //ld r30, y      ; а теперь загружем старший байт адреса
; 0000 023C //ldd r31, y+1
; 0000 023D //CLR R22
; 0000 023E //LDI  R22,LOW(3)
; 0000 023F //out  0x37,R22
; 0000 0240 //cli
; 0000 0241 //spm
; 0000 0242 ld r30, y+      ; а теперь загружем старший байт адреса
; 0000 0243 ldd r31, y+1
; 0000 0244 CLR R22
