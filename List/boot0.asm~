;int writePageToFlash()
; 0000 0259 {
_writePageToFlash:
; .FSTART _writePageToFlash
; 0000 025A  #asm
; 0000 025B   ;-the routine writes one page of data from RAM to Flash
  ;-the routine writes one page of data from RAM to Flash
; 0000 025C ; the first data location in RAM is pointed to by the Y pointer
; the first data location in RAM is pointed to by the Y pointer
; 0000 025D ; the first data location in Flash is pointed to by the Z-pointer
; the first data location in Flash is pointed to by the Z-pointer
; 0000 025E ;-error handling is not included
;-error handling is not included
; 0000 025F ;-the routine must be placed inside the Boot space
;-the routine must be placed inside the Boot space
; 0000 0260 ; (at least the Do_spm sub routine). Only code inside NRWW section can
; (at least the Do_spm sub routine). Only code inside NRWW section can
; 0000 0261 ; be read during Self-Programming (Page Erase and Page Write).
; be read during Self-Programming (Page Erase and Page Write).
; 0000 0262 ;-registers used: r0, r1, temp1 (r16), temp2 (r17), looplo (r24),
;-registers used: r0, r1, temp1 (r16), temp2 (r17), looplo (r24),
; 0000 0263 ; loophi (r25), spmcrval (r20)
; loophi (r25), spmcrval (r20)
; 0000 0264 ; storing and restoring of registers is not included in the routine
; storing and restoring of registers is not included in the routine
; 0000 0265 ; register usage can be optimized at the expense of code size
; register usage can be optimized at the expense of code size
; 0000 0266 ;-It is assumed that either the interrupt table is moved to the Boot
;-It is assumed that either the interrupt table is moved to the Boot
; 0000 0267 ; loader section or that the interrupts are disabled.
; loader section or that the interrupts are disabled.
; 0000 0268 .equ PAGESIZEB = 64*2 ;PAGESIZEB is page size in BYTES, not words
.equ PAGESIZEB = 64*2 ;PAGESIZEB is page size in BYTES, not words
; 0000 0269 .def temp1 = r16
.def temp1 = r16
; 0000 026A .def temp2 = r17
.def temp2 = r17
; 0000 026B .def looplo = r24
.def looplo = r24
; 0000 026C .def loophi = r25
.def loophi = r25
; 0000 026D .def spmcrval = r20
.def spmcrval = r20
; 0000 026E .def ZH = r31
.def ZH = r31
; 0000 026F .def ZL = r30
.def ZL = r30
; 0000 0270 .def YL = r28
.def YL = r28
; 0000 0271 .def YH = r29
.def YH = r29
; 0000 0272 //.org SMALLBOOTSTART
//.org SMALLBOOTSTART
; 0000 0273 Write_page:
Write_page:
; 0000 0274 ; Page Erase
; Page Erase
; 0000 0275 ldi spmcrval, (1<<PGERS) | (1<<SPMEN)
ldi spmcrval, (1<<PGERS) | (1<<SPMEN)
; 0000 0276 call Do_spm
call Do_spm
; 0000 0277 ; re-enable the RWW section
; re-enable the RWW section
; 0000 0278 ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
; 0000 0279 call Do_spm
call Do_spm
; 0000 027A ; transfer data from RAM to Flash page buffer
; transfer data from RAM to Flash page buffer
; 0000 027B ldi looplo, low(PAGESIZEB) ;init loop variable
ldi looplo, low(PAGESIZEB) ;init loop variable
; 0000 027C ldi loophi, high(PAGESIZEB) ;not required for PAGESIZEB<=256
ldi loophi, high(PAGESIZEB) ;not required for PAGESIZEB<=256
; 0000 027D Wrloop:
Wrloop:
; 0000 027E ld r0, Y+
ld r0, Y+
; 0000 027F ld r1, Y+
ld r1, Y+
; 0000 0280 ldi spmcrval, (1<<SPMEN)
ldi spmcrval, (1<<SPMEN)
; 0000 0281 call Do_spm
call Do_spm
; 0000 0282 adiw ZH:ZL, 2
adiw ZH:ZL, 2
; 0000 0283 //adiw ZL,2
//adiw ZL,2
; 0000 0284 sbiw loophi:looplo, 2 ;use subi for PAGESIZEB<=256
sbiw loophi:looplo, 2 ;use subi for PAGESIZEB<=256
; 0000 0285 brne Wrloop
brne Wrloop
; 0000 0286 ; execute Page Write
; execute Page Write
; 0000 0287 subi ZL, low(PAGESIZEB) ;restore pointer
subi ZL, low(PAGESIZEB) ;restore pointer
; 0000 0288 sbci ZH, high(PAGESIZEB) ;not required for PAGESIZEB<=256
sbci ZH, high(PAGESIZEB) ;not required for PAGESIZEB<=256
; 0000 0289 ldi spmcrval, (1<<PGWRT) | (1<<SPMEN)
ldi spmcrval, (1<<PGWRT) | (1<<SPMEN)
; 0000 028A call Do_spm
call Do_spm
; 0000 028B ; re-enable the RWW section
; re-enable the RWW section
; 0000 028C ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
; 0000 028D call Do_spm
call Do_spm
; 0000 028E ; read back and check, optional
; read back and check, optional
; 0000 028F ldi looplo, low(PAGESIZEB) ;init loop variable
ldi looplo, low(PAGESIZEB) ;init loop variable
; 0000 0290 ldi loophi, high(PAGESIZEB) ;not required for PAGESIZEB<=256
ldi loophi, high(PAGESIZEB) ;not required for PAGESIZEB<=256
; 0000 0291 subi YL, low(PAGESIZEB) ;restore pointer
subi YL, low(PAGESIZEB) ;restore pointer
; 0000 0292 sbci YH, high(PAGESIZEB)
sbci YH, high(PAGESIZEB)
; 0000 0293 Rdloop:
Rdloop:
; 0000 0294 lpm r0, Z+
lpm r0, Z+
; 0000 0295 ld r1, Y+
ld r1, Y+
; 0000 0296 cpse r0, r1
cpse r0, r1
; 0000 0297 jmp Error
jmp Error
; 0000 0298 sbiw loophi:looplo, 1 ;use subi for PAGESIZEB<=256
sbiw loophi:looplo, 1 ;use subi for PAGESIZEB<=256
; 0000 0299 brne Rdloop
brne Rdloop
; 0000 029A ; return to RWW section
; return to RWW section
; 0000 029B ; verify that RWW section is safe to read
; verify that RWW section is safe to read
; 0000 029C Return:
Return:
; 0000 029D in temp1, SPMCSR
in temp1, SPMCSR
; 0000 029E sbrs temp1, RWWSB ; If RWWSB is set, the RWW section is not ready yet
sbrs temp1, RWWSB ; If RWWSB is set, the RWW section is not ready yet
; 0000 029F ret
ret
; 0000 02A0 ; re-enable the RWW section
; re-enable the RWW section
; 0000 02A1 ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
ldi spmcrval, (1<<RWWSRE) | (1<<SPMEN)
; 0000 02A2 call Do_spm
call Do_spm
; 0000 02A3 rjmp Return
rjmp Return
; 0000 02A4 Do_spm:
Do_spm:
; 0000 02A5 ; check for previous SPM complete
; check for previous SPM complete
; 0000 02A6 Wait_spm:
Wait_spm:
; 0000 02A7 in temp1, SPMCSR
in temp1, SPMCSR
; 0000 02A8 sbrc temp1, SPMEN
sbrc temp1, SPMEN
; 0000 02A9 rjmp Wait_spm
rjmp Wait_spm
; 0000 02AA ; input: spmcrval determines SPM action
; input: spmcrval determines SPM action
; 0000 02AB ; disable interrupts if enabled, store status
; disable interrupts if enabled, store status
; 0000 02AC in temp2, SREG
in temp2, SREG
; 0000 02AD cli
cli
; 0000 02AE ; check that no EEPROM write access is present
; check that no EEPROM write access is present
; 0000 02AF Wait_ee:
Wait_ee:
; 0000 02B0 sbic EECR, EEPE
sbic EECR, EEPE
; 0000 02B1 rjmp Wait_ee
rjmp Wait_ee
; 0000 02B2 ; SPM timed sequence
; SPM timed sequence
; 0000 02B3 out SPMCSR, spmcrval
out SPMCSR, spmcrval
; 0000 02B4 spm
spm
; 0000 02B5 ; restore SREG (to enable interrupts if originally enabled)
; restore SREG (to enable interrupts if originally enabled)
; 0000 02B6 out SREG, temp2
out SREG, temp2
; 0000 02B7 ret
ret
; 0000 02B8  #endasm
; 0000 02B9 }
	RET
; .FEND
;
;void main(void)
; 0000 02BC {
_main:
; .FSTART _main
; 0000 02BD // Declare your local variables here
; 0000 02BE int i=0,j=0;
; 0000 02BF system_init();
;	i -> R16,R17
;	j -> R18,R19
	__GETWRN 16,17,0
	__GETWRN 18,19,0
	RCALL _system_init
; 0000 02C0 
; 0000 02C1 //SPMCSR = 0x00;
; 0000 02C2 #asm("wdr")
	wdr
; 0000 02C3 //
; 0000 02C4 //for(i=0; i < 64;i++)
; 0000 02C5 //{
; 0000 02C6 //
; 0000 02C7 //formTmpBuffer(i);
; 0000 02C8 //
; 0000 02C9 //}
; 0000 02CA //erasePageFromMemory(0x0000);
; 0000 02CB 
; 0000 02CC //writePageToMemory(0x0000);
; 0000 02CD 
; 0000 02CE 
; 0000 02CF //sensor_address=Parameter_bank[14];
; 0000 02D0 writePageToFlash();
	RCALL _writePageToFlash
; 0000 02D1 #asm("sei")
	sei
; 0000 02D2 delay_ms(10);
	LDI  R26,LOW(10)
	LDI  R27,0
	RCALL _delay_ms
; 0000 02D3 Parameter_bank[0] = 10;
	LDI  R26,LOW(_Parameter_bank)
	LDI  R27,HIGH(_Parameter_bank)
	LDI  R30,LOW(10)
	RCALL __EEPROMWRB
; 0000 02D4         RxEn;
	RCALL SUBOPT_0x0
; 0000 02D5         Recieve;
	SBI  0xB,3
; 0000 02D6 
; 0000 02D7 
; 0000 02D8         while (1)
_0x6A:
; 0000 02D9               {
; 0000 02DA                 #asm("wdr")
	wdr
; 0000 02DB                 if(message_recieved)
	SBIS 0x1E,4
	RJMP _0x6D
; 0000 02DC                 {
; 0000 02DD                  //transmit_HART();
; 0000 02DE                 }
; 0000 02DF 
; 0000 02E0               }
_0x6D:
	RJMP _0x6A
_0x6C:
; 0000 02E1 }
_0x6E:
	RJMP _0x6E
; .FEND

	.ESEG
_Parameter_bank:
	.DB  0x0,0x56,0xB3,0x4
	.DB  0x1,0x1,0x1,0x21
	.DB  0x0,0x0,0xBF,0xBC
	.DB  0x6D,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x2,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x1
	.DB  0x2,0x3,0x42,0x48
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x3C,0x23
	.DB  0xD7,0xA,0x0,0x0
	.DB  0x0,0x0,0xA0,0x41
	.DB  0x0,0x0,0x0,0x0
	.DB  0xF0,0xF,0x5,0x0
	.DB  0x1,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x80
	.DB  0x40,0x0,0x0,0xA0
	.DB  0x41,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0,0x0,0x0
	.DB  0x0,0x0

	.DSEG
_rx_buffer0:
	.BYTE 0x40
_command_rx_val:
	.BYTE 0x1
_preambula_bytes_rec:
	.BYTE 0x1
_bytes_quantity_ans:
	.BYTE 0x1
_Command_data:
	.BYTE 0x19
_preambula_bytes:
	.BYTE 0x1
_tx_buffer0:
	.BYTE 0x40
_tx_rd_index0:
	.BYTE 0x1
_tx_counter0:
	.BYTE 0x1

	.CSEG
;OPTIMIZER ADDED SUBROUTINE, CALLED 2 TIMES, CODE SIZE REDUCTION:3 WORDS
SUBOPT_0x0:
	LDS  R30,193
	ANDI R30,LOW(0xC0)
	ORI  R30,0x10
	STS  193,R30
	RET

;OPTIMIZER ADDED SUBROUTINE, CALLED 2 TIMES, CODE SIZE REDUCTION:1 WORDS
SUBOPT_0x1:
	LDI  R30,LOW(0)
	STS  110,R30
	RET


	.CSEG
_delay_ms:
	adiw r26,0
	breq __delay_ms1
__delay_ms0:
	__DELAY_USB 0x99
	wdr
	sbiw r26,1
	brne __delay_ms0
__delay_ms1:
	ret

__EEPROMWRB:
	SBIS EECR,EEWE
	RJMP __EEPROMWRB1
	WDR
	RJMP __EEPROMWRB
__EEPROMWRB1:
	IN   R25,SREG
	CLI
	OUT  EEARL,R26
	OUT  EEARH,R27
	SBI  EECR,EERE
	IN   R24,EEDR
	CP   R30,R24
	BREQ __EEPROMWRB0
	OUT  EEDR,R30
	SBI  EECR,EEMWE
	SBI  EECR,EEWE
__EEPROMWRB0:
	OUT  SREG,R25
	RET

;END OF CODE MARKER
__END_OF_CODE:
