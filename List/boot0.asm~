;// USART Transmitter interrupt service routine
;interrupt [USART_TXC] void usart_tx_isr(void)//передача по USART соответственно
; 0000 008D {
_usart_tx_isr:
; .FSTART _usart_tx_isr
	ST   -Y,R26
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
; 0000 008E #asm("cli")
	cli
; 0000 008F 
; 0000 0090 if (tx_counter0)
	LDS  R30,_tx_counter0
	CPI  R30,0
	BRNE PC+2
	RJMP _0x8
; 0000 0091    {
; 0000 0092    --tx_counter0;
	SUBI R30,LOW(1)
	STS  _tx_counter0,R30
; 0000 0093 
; 0000 0094    UDR0=tx_buffer0[tx_rd_index0];
	LDS  R30,_tx_rd_index0
	LDI  R31,0
	SUBI R30,LOW(-_tx_buffer0)
	SBCI R31,HIGH(-_tx_buffer0)
	LD   R30,Z
	STS  198,R30
; 0000 0095 
; 0000 0096    if (++tx_rd_index0 == TX_BUFFER_SIZE0) tx_rd_index0=0;
	LDS  R26,_tx_rd_index0
	SUBI R26,-LOW(1)
	STS  _tx_rd_index0,R26
	CPI  R26,LOW(0x40)
	BREQ PC+2
	RJMP _0x9
	LDI  R30,LOW(0)
	STS  _tx_rd_index0,R30
; 0000 0097    };
_0x9:
_0x8:
; 0000 0098    #asm("sei")
	sei
; 0000 0099 }
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	LD   R26,Y+
	RETI
; .FEND
;// External Interrupt 0 service routine
;interrupt [EXT_INT0] void ext_int0_isr(void)//первоначально прерывание работает по нарастающему уровню (set_rising_edge_ ...
; 0000 009C //изменено, таймер, отсчитывающий задержку, сейчас не активен, пользуемся только OCD ногой модема
; 0000 009D {
_ext_int0_isr:
; .FSTART _ext_int0_isr
	ST   -Y,R26
	ST   -Y,R30
	IN   R30,SREG
	ST   -Y,R30
; 0000 009E if(EICRA==0x03)                    //если сработало прерывание по верхнему уровню, то переключаемся на отлов нижнего уро ...
	LDS  R26,105
	CPI  R26,LOW(0x3)
	BREQ PC+2
	RJMP _0xA
; 0000 009F                 {
; 0000 00A0                 Recieve;
	SBI  0xB,3
; 0000 00A1                 RxEn;
	RCALL SUBOPT_0x0
; 0000 00A2                 //wait_stopOCD;
; 0000 00A3                 //start_wait_Rx_timer;
; 0000 00A4                 //disable_eints;
; 0000 00A5                 wait_stopOCD;           //EICRA=0x00
	LDI  R30,LOW(0)
	STS  105,R30
; 0000 00A6                 message_recieved=0;
	CBI  0x1E,4
; 0000 00A7                 //mono_channel_mode;
; 0000 00A8                 }
; 0000 00A9 else
	RJMP _0xF
_0xA:
; 0000 00AA                 {
; 0000 00AB                 //Transmit;
; 0000 00AC 
; 0000 00AD                 //stop_wait_Rx_timer;
; 0000 00AE                 wait_startOCD;            //EICRA=0x03
	LDI  R30,LOW(3)
	STS  105,R30
; 0000 00AF                 disable_uart;             //отключаем USART, переходим в режим приема
	LDI  R30,LOW(192)
	STS  193,R30
; 0000 00B0                 message_recieved=1;
	SBI  0x1E,4
; 0000 00B1 
; 0000 00B2                 }
_0xF:
; 0000 00B3 //start_check_OCD_timer;//стартуем таймер отсчитывающий задержку 3.33 мс (4 цикла при минимальной частоте 1200Гц)
; 0000 00B4 
; 0000 00B5 }
	LD   R30,Y+
	OUT  SREG,R30
	LD   R30,Y+
	LD   R26,Y+
	RETI
; .FEND
;
