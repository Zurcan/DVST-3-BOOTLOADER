;interrupt [USART_RXC] void usart_rx_isr(void)//прием по USART
; 0000 006E {

	.CSEG
_usart_rx_isr:
; .FSTART _usart_rx_isr
	ST   -Y,R30
	ST   -Y,R31
	IN   R30,SREG
	ST   -Y,R30
; 0000 006F 
; 0000 0070 char status,data;
; 0000 0071 #asm("cli")
	ST   -Y,R17
	ST   -Y,R16
;	status -> R17
;	data -> R16
	cli
; 0000 0072 status=UCSR0A;
	LDS  R17,192
; 0000 0073 
; 0000 0074 data=UDR0;
	LDS  R16,198
; 0000 0075 //#asm("sei")
; 0000 0076 
; 0000 0077 if ((status & (FRAMING_ERROR | PARITY_ERROR | DATA_OVERRUN))==0)//если нет ошибок, то читаем данные в буфере USART
	MOV  R30,R17
	ANDI R30,LOW(0x1C)
	BREQ PC+2
	RJMP _0x3
; 0000 0078    {
; 0000 0079    rx_buffer0[rx_wr_index0]=data;
	MOV  R30,R5
	LDI  R31,0
	SUBI R30,LOW(-_rx_buffer0)
	SBCI R31,HIGH(-_rx_buffer0)
	ST   Z,R16
; 0000 007A    if (++rx_wr_index0 == RX_BUFFER_SIZE0) rx_wr_index0=0;
	INC  R5
	LDI  R30,LOW(64)
	CP   R30,R5
	BREQ PC+2
	RJMP _0x4
	CLR  R5
; 0000 007B    if (++rx_counter0 == RX_BUFFER_SIZE0)
_0x4:
	INC  R7
	LDI  R30,LOW(64)
	CP   R30,R7
	BREQ PC+2
	RJMP _0x5
; 0000 007C       {
; 0000 007D       rx_counter0=0;
	CLR  R7
; 0000 007E       rx_buffer_overflow0=1;
	SBI  0x1E,0
; 0000 007F 
; 0000 0080      };
_0x5:
; 0000 0081    };
_0x3:
; 0000 0082  #asm("sei")
	sei
; 0000 0083 }
	LD   R16,Y+
	LD   R17,Y+
	LD   R30,Y+
	OUT  SREG,R30
	LD   R31,Y+
	LD   R30,Y+
	RETI
; .FEND
;// USART Transmitter buffer
;#define TX_BUFFER_SIZE0 64
;char tx_buffer0[TX_BUFFER_SIZE0];
;
;unsigned char tx_wr_index0,tx_rd_index0,tx_counter0;
