;void system_init(){
; 0000 01A2 void system_init(){
_system_init:
; .FSTART _system_init
; 0000 01A3 #asm("wdr")
	wdr
; 0000 01A4 WDTCSR=0x38;
	LDI  R30,LOW(56)
	STS  96,R30
; 0000 01A5 WDTCSR=0x0E;
	LDI  R30,LOW(14)
	STS  96,R30
; 0000 01A6 // Crystal Oscillator division factor: 1
; 0000 01A7 /*#pragma optsize-
; 0000 01A8 CLKPR=0x80;
; 0000 01A9 CLKPR=0x00;
; 0000 01AA #ifdef _OPTIMIZE_SIZE_
; 0000 01AB #pragma optsize+
; 0000 01AC #endif
; 0000 01AD   */
; 0000 01AE // Input/Output Ports initialization
; 0000 01AF // Port B initialization
; 0000 01B0 // Func7=In Func6=In Func5=Out Func4=In Func3=Out Func2=Out Func1=In Func0=In
; 0000 01B1 // State7=T State6=T State5=0 State4=T State3=0 State2=0 State1=T State0=T
; 0000 01B2 PORTB=0x00;
	LDI  R30,LOW(0)
	OUT  0x5,R30
; 0000 01B3 DDRB=0x2c;
	LDI  R30,LOW(44)
	OUT  0x4,R30
; 0000 01B4 
; 0000 01B5 // Port C initialization
; 0000 01B6 // Func6=In Func5=In Func4=In Func3=In Func2=In Func1=In Func0=In
; 0000 01B7 // State6=T State5=T State4=T State3=T State2=T State1=T State0=T
; 0000 01B8 PORTC=0x00;
	LDI  R30,LOW(0)
	OUT  0x8,R30
; 0000 01B9 DDRC=0x00;
	OUT  0x7,R30
; 0000 01BA 
; 0000 01BB // Port D initialization
; 0000 01BC // Func7=In Func6=In Func5=In Func4=In Func3=In Func2=In Func1=In Func0=In
; 0000 01BD // State7=T State6=T State5=T State4=T State3=T State2=T State1=T State0=T
; 0000 01BE //PORTD=0x00;
; 0000 01BF DDRD.3=1;
	SBI  0xA,3
; 0000 01C0 PORTD.3=1;
	SBI  0xB,3
; 0000 01C1 DDRD.6=1;
	SBI  0xA,6
; 0000 01C2 DDRD.7=1;
	SBI  0xA,7
; 0000 01C3 PORTD.6=0;
	CBI  0xB,6
; 0000 01C4 PORTD.7=0;
	CBI  0xB,7
; 0000 01C5 // Timer/Counter 0 initialization
; 0000 01C6 // Clock source: System Clock
; 0000 01C7 // Clock value: Timer 0 Stopped
; 0000 01C8 // Mode: Normal top=FFh
; 0000 01C9 // OC0A output: Disconnected
; 0000 01CA // OC0B output: Disconnected
; 0000 01CB //TCCR0A=0x00;
; 0000 01CC //TCCR0B=0x04;
; 0000 01CD //TCNT0=0xA5;
; 0000 01CE //OCR0A=0x00;
; 0000 01CF //OCR0B=0x00;
; 0000 01D0 stop_wait_Rx_timer;
	RCALL SUBOPT_0x1
	OUT  0x24,R30
	LDI  R30,LOW(0)
	OUT  0x25,R30
	OUT  0x26,R30
; 0000 01D1 /*USART predefinition: 1200 baud rate, tx enable, all interrutpts enabled 8bit buffer*/
; 0000 01D2 UCSR0A=0x00;
	STS  192,R30
; 0000 01D3 UCSR0B=0xc0;
	LDI  R30,LOW(192)
	STS  193,R30
; 0000 01D4 UCSR0C=0x06;
	LDI  R30,LOW(6)
	STS  194,R30
; 0000 01D5 UBRR0H=0x00;
	LDI  R30,LOW(0)
	STS  197,R30
; 0000 01D6 UBRR0L=0x17;
	LDI  R30,LOW(23)
	STS  196,R30
; 0000 01D7 
; 0000 01D8 // Timer/Counter 1 initialization
; 0000 01D9 // Clock source: System Clock
; 0000 01DA // Clock value: Timer 1 Stopped
; 0000 01DB // Mode: Normal top=FFFFh
; 0000 01DC // OC1A output: Discon.
; 0000 01DD // OC1B output: Discon.
; 0000 01DE // Noise Canceler: Off
; 0000 01DF // Input Capture on Falling Edge
; 0000 01E0 // Timer 1 Overflow Interrupt: Off
; 0000 01E1 // Input Capture Interrupt: Off
; 0000 01E2 // Compare A Match Interrupt: Off
; 0000 01E3 // Compare B Match Interrupt: Off
; 0000 01E4 TCCR1A=0x00;
	LDI  R30,LOW(0)
	STS  128,R30
; 0000 01E5 TCCR1B=0x00;
	STS  129,R30
; 0000 01E6 TCNT1H=0x00;
	STS  133,R30
; 0000 01E7 TCNT1L=0x00;
	STS  132,R30
; 0000 01E8 ICR1H=0x00;
	STS  135,R30
; 0000 01E9 ICR1L=0x00;
	STS  134,R30
; 0000 01EA OCR1AH=0x00;
	STS  137,R30
; 0000 01EB OCR1AL=0x00;
	STS  136,R30
; 0000 01EC OCR1BH=0x00;
	STS  139,R30
; 0000 01ED OCR1BL=0x00;
	STS  138,R30
; 0000 01EE 
; 0000 01EF // Timer/Counter 2 initialization
; 0000 01F0 // Clock source: System Clock
; 0000 01F1 // Clock value: Timer 2 Stopped
; 0000 01F2 // Mode: Normal top=FFh
; 0000 01F3 // OC2A output: Disconnected
; 0000 01F4 // OC2B output: Disconnected
; 0000 01F5 ASSR=0x00;
	STS  182,R30
; 0000 01F6 TCCR2A=0x00;
	STS  176,R30
; 0000 01F7 TCCR2B=0x00;
	STS  177,R30
; 0000 01F8 TCNT2=0x00;
	STS  178,R30
; 0000 01F9 OCR2A=0x00;
	STS  179,R30
; 0000 01FA OCR2B=0x00;
	STS  180,R30
; 0000 01FB 
; 0000 01FC // External Interrupt(s) initialization
; 0000 01FD // INT0: On
; 0000 01FE // INT0 Mode: Any change
; 0000 01FF // INT1: Off
; 0000 0200 // Interrupt on any change on pins PCINT0-7: Off
; 0000 0201 // Interrupt on any change on pins PCINT8-14: Off
; 0000 0202 // Interrupt on any change on pins PCINT16-23: Off
; 0000 0203 wait_startOCD;
	LDI  R30,LOW(3)
	STS  105,R30
; 0000 0204 EIMSK=0x01;
	LDI  R30,LOW(1)
	OUT  0x1D,R30
; 0000 0205 EIFR=0x01;
	OUT  0x1C,R30
; 0000 0206 PCICR=0x00;
	LDI  R30,LOW(0)
	STS  104,R30
; 0000 0207 
; 0000 0208 
; 0000 0209 // Timer/Counter 0 Interrupt(s) initialization
; 0000 020A TIMSK0=0x00;
	RCALL SUBOPT_0x1
; 0000 020B // Timer/Counter 1 Interrupt(s) initialization
; 0000 020C TIMSK1=0x00;
	STS  111,R30
; 0000 020D // Timer/Counter 2 Interrupt(s) initialization
; 0000 020E TIMSK2=0x00;
	LDI  R30,LOW(0)
	STS  112,R30
; 0000 020F 
; 0000 0210 // Analog Comparator initialization
; 0000 0211 // Analog Comparator: Off
; 0000 0212 // Analog Comparator Input Capture by Timer/Counter 1: Off
; 0000 0213 ACSR=0x80;
	LDI  R30,LOW(128)
	OUT  0x30,R30
; 0000 0214 ADCSRB=0x00;
	LDI  R30,LOW(0)
	STS  123,R30
; 0000 0215 
; 0000 0216 }
	RET
; .FEND
;
;//int formTmpBuffer(unsigned char wordAddress, int word)
;int formTmpBuffer( int word)
; 0000 021A {
; 0000 021B #asm
;	word -> Y+0
; 0000 021C cli
; 0000 021D wdr
; 0000 021E ld r0, y;
; 0000 021F ldd r1, y+1;
; 0000 0220 ld r30, y      ; а теперь загружем старший байт адреса
; 0000 0221 ldd r31, y+1
; 0000 0222 LDI  R22,0x01
; 0000 0223 OUT  0x37,R22
; 0000 0224 spm
; 0000 0225 #endasm
; 0000 0226 
; 0000 0227 }
